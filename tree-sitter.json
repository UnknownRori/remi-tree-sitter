{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "remi",
      "camelcase": "Remi",
      "title": "Remi",
      "scope": "source.remi",
      "file-types": [
        "remi"
      ],
      "injection-regex": "^remi$",
      "class-name": "TreeSitterRemi"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Remi-lang Tree Sitter",
    "authors": [
      {
        "name": "UnknownRori",
        "email": "unknownrori@proton.me",
        "url": "https://unknownrori.vercel.app/"
      }
    ],
    "links": {
      "repository": "https://github.com/UnknownRori/remi-tree-sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}